	draggable="true" >
		<div class="icon--holder">
			<img src="https://media.flaticon.com/img/loader.gif" data-src="{{icoImg}}" class="collectionIconImage" onload="Widget.renderSVG(this)" data-premium="{{premium}}">
			<span class="personal-icon--text">custom</span>
			<div class="overlay" aria-hidden="true">
				<a href="https://www.flaticon.com/icon/{{id}}" class="view link-icon-detail" title="{{name}}"></a>
				<button class="add-remove btn flaticon-collections_remove nostyle"></button>
			</div>

	        <div class="popover icon-options hToggleRender collectionIcon">
	            <div class="popover-container">
	                <button class="btn popover-button">
	                    <i class="flaticon-dots"></i>
	                </button>
	                <div class="popover-content">
	                    <ul>
	                    	{{#if not_personal}}
	                        <li><a href="https://www.flaticon.com/icon/{{id}}" class="" title="{{name}}" data-id="{{#if id}}{{id}}{{/if}}"><i class="flaticon-eye"></i><span> View detail</span></a></li>
	                    	{{/if}}
	                        <li><a href="#" class="collection_add-remove"><b><i class="flaticon-collections_remove"></i></b><span> Remove from collection</span></a></li>
