<script>

	var download_types = ['icon','pack','collection'];
	var ga_arguments_premium, mix_panel_json_premium, ga_arguments_copy, ga_credit_arguments_copy;
	var licence;

	function check_modal_download(type,element_type) {
			
		if (download_types.indexOf(type)==-1) {
			return;
		}

		var modal = document.querySelector('div.modal-download');
		var modal_credit = document.querySelector('div.modal-credit');

		if (modal) {
			// SHOW & HIDE ELEMENTS
			modal_download_toogle_all(modal,modal_credit,type);

			// ICON
			if (type == 'icon') {
				check_modal_download_icon(modal,modal_credit,element_type);
			}

			// COLLECTION
			if (type == 'collection') {
				check_modal_download_collection(modal,modal_credit);
			}

			// PACK
			if (type == 'pack') {
				check_modal_download_pack(modal,modal_credit);
			}

			document.getElementById('modal-download').checked = true;
			return;
		}	
	}

	function modal_download_toogle_all(modal,modal_credit,type) 
	{				
		modal.classList.add('modal-'+type);		
				
		modal.querySelector('#download-premium').classList.add('modal-'+type);
		modal.querySelector('#download-free').classList.add('modal-'+type);

		modal.querySelector('.modal-body').classList.remove('nopremium');

		//  SHOW ALL
		[].forEach.call(modal.querySelectorAll('.hidden'), function(all_element) {
			if (all_element && all_element.classList.contains('hidden')) {
				all_element.classList.remove('hidden');
			} 
		});

		[].forEach.call(modal_credit.querySelectorAll('.hidden'), function(all_element) {
			if (all_element && all_element.classList.contains('hidden')) {
				all_element.classList.remove('hidden');
			} 
		});

		// GENERAL HIDE
		download_types.forEach(function (t) {
			if (t != type) {

				modal.classList.remove('modal-'+t);		
				modal.querySelector('#download-premium').classList.remove('modal-'+t);
				modal.querySelector('#download-free').classList.remove('modal-'+t);	
							
				[].forEach.call(modal.querySelectorAll('.modal-'+t), function(element_hide) {
					element_hide.classList.add('hidden');
				});
			}

		});
	}

	/**
	*
	*/
	function check_modal_download_icon(modal, modal_credit, element_type) 
	{
		
		ga_arguments_premium = ['send', 'event', 'Detail View', 'Premium Download'];
		mix_panel_json_premium = {'source': 'not-premium-detail', 'download-type': 'single', 'website': 'flaticon'};
		ga_arguments_copy = ['send', 'event', 'Detail View', 'Select Attribution Link'];
		ga_credit_arguments_copy = ['send', 'event', 'Detail View', 'Select Credit Attribution Link'];

		document.querySelector('.modal #download-free').addEventListener('click',download_final);
		document.querySelector('.modal #copy-attributtion').addEventListener('click',apply_ga_attributtion);
		document.querySelector('.modal-credit #copy-attributtion-credit').addEventListener('click',apply_ga_attributtion_credit);
		document.querySelector('.modal #download-premium').addEventListener('click',apply_ga_download_premium);

		var download_form = document.getElementById('download-form');

		if (element_type) {

			// DESDE EL LISTADO
		    modal.querySelector('p.filename img').setAttribute('src', element_type.querySelector('img').getAttribute('src'));

			modal_credit.querySelector('.col--img').classList.remove('hidden');
		    modal_credit.querySelector('.col--img').style.backgroundImage = "url('"+ element_type.querySelector('img').getAttribute('src') +"')";

		    modal.querySelector('p.filename span.description').innerHTML = element_type.dataset.name;
		    modal.querySelector('p.author span.description').innerHTML = 'By ' + element_type.dataset.author_name;
		    modal.querySelector('p.author img').setAttribute('src', element_type.dataset.author_avatar);

				if (element_type.dataset.selection == 0) {
					modal.querySelector('.modal-body').classList.add('nopremium');
					modal.querySelector('.column-premium').style.display = 'none';
				} else {
					modal.querySelector('.column-premium').style.display = 'block';
				}

		    licence = '<div>Icons made by <a href="'+element_type.dataset.author_website+
		    '" title="'+element_type.dataset.author_name+'">'+element_type.dataset.author_name+'</a> from <a href="'+APP_URL+'" \
		    title="Flaticon">www.flaticon.com</a> is licensed by <a href="http://creativecommons.org/licenses/by/3.0/" \
		    title="Creative Commons BY 3.0" target="_blank">CC 3.0 BY</a></div>';

		} else {

			// DESDE EL DETALLE
			if (download_form) {

				var icon_info = (document.querySelector('#detail')) ? document.getElementById('detail') : document.getElementById('detail-content');

				var description = icon_info.querySelector('h1 .title').innerHTML;
				modal.querySelector('.filename .description').innerHTML = description;
				modal.querySelector('.filename img').alt = description;

				var main_img = icon_info.querySelector('.main-icon img');
				modal.querySelector('.filename img').src = main_img.src;

				modal_credit.querySelector('.col--img').classList.remove('hidden');
		    	modal_credit.querySelector('.col--img').style.backgroundImage = "url('"+ main_img.src +"')";

				var author_link = icon_info.querySelector('.author a.avatar'); 
				var author_img = icon_info.querySelector('.author a.avatar img');

				modal.querySelector('.author img').src =  author_img.src;
				modal.querySelector('.author .description').innerHTML = author_link.title;


				if (download_form.selection.value == 0) {
					modal.querySelector('.modal-body').classList.add('nopremium');
					modal.querySelector('.column-premium').style.display = 'none';
				} else {
					modal.querySelector('.column-premium').style.display = 'block';
				}

			    licence = '<div>Icons made by <a href="'+author_link.href+
			    '" title="'+author_link.title+'">'+author_link.title+'</a> from <a href="'+APP_URL+'" \
			    title="Flaticon">www.flaticon.com</a> is licensed by <a href="http://creativecommons.org/licenses/by/3.0/" \
			    title="Creative Commons BY 3.0" target="_blank">CC 3.0 BY</a></div>';

			}			    

					
		}
		
		modal.querySelector('#input_atributtion_text').setAttribute('value', licence);    
		modal_credit.querySelector('#input_atributtion_credit_text').setAttribute('value', licence);	
		

	}

	/**
	*
	*/
	function check_modal_download_pack(modal, modal_credit) 
	{
		ga_arguments_premium = ['send', 'event', 'Pack', 'Premium Download'];
		mix_panel_json_premium = {'source': 'not-premium-detail', 'download-type': 'pack', 'website': 'flaticon'};
		ga_arguments_copy = ['send', 'event', 'Pack', 'Select Attribution Link'];
		ga_credit_arguments_copy = ['send', 'event', 'Pack', 'Select Credit Attribution Link'];

		document.querySelector('.modal #download-free').addEventListener('click', download_pack);
		document.querySelector('.modal #copy-attributtion').addEventListener('click',apply_ga_attributtion);
		document.querySelector('.modal-credit #copy-attributtion-credit').addEventListener('click',apply_ga_attributtion_credit);
		document.querySelector('.modal #download-premium').addEventListener('click',apply_ga_download_premium);


		var author = document.querySelector('.pack-header .author a');
		var author_link = document.querySelector('.pack-header .author a + a');
		licence = '<div>Icons made by <a href="'+author_link.href+
		    '" title="'+author.title+'">'+author.title+'</a> from <a href="'+APP_URL+'" \
		    title="Flaticon">www.flaticon.com</a> is licensed by <a href="http://creativecommons.org/licenses/by/3.0/" \
		    title="Creative Commons BY 3.0" target="_blank">CC 3.0 BY</a></div>';
		

		modal.querySelector('#input_atributtion_text').setAttribute('value', licence);    
		modal_credit.querySelector('#input_atributtion_credit_text').setAttribute('value', licence);

		modal.querySelector('.pack_name').innerHTML = document.querySelector('h1 .title').innerHTML;

		var sprite_pack = document.querySelector('meta[property="og:image"]');
		modal_credit.querySelector('.col--img').classList.remove('hidden');
		modal_credit.querySelector('.col--img').style.backgroundImage = "url('"+ sprite_pack.content +"')";

	}

	/**
	*
	*/
	function check_modal_download_collection(modal, modal_credit) 
	{
		ga_arguments_premium = ['send', 'event', 'Collection', 'Premium Download'];
		mix_panel_json_premium = {'source': 'not-premium-detail', 'download-type': 'collection', 'website': 'flaticon'};
		ga_arguments_copy = null;
		ga_credit_arguments_copy = null;

		document.querySelector('.modal #download-free').addEventListener('click',download_collection);
		document.querySelector('.modal #download-premium').addEventListener('click',apply_ga_download_premium);

		modal.querySelector('.author').classList.add('hidden');
		modal.querySelector('.copy-wrapper').classList.add('hidden');

		modal_credit.querySelector('.copy-wrapper').classList.add('hidden');
		modal_credit.querySelector('.col--img').classList.add('hidden');
		
	}

	/**
	*
	*/
	function apply_ga_attributtion() {
		document.querySelector('.modal #input_atributtion_text').select();
		document.execCommand('copy');
		if (ga_arguments_copy) {
			ga(ga_arguments_copy[0], ga_arguments_copy[1], ga_arguments_copy[2], ga_arguments_copy[3]);
		}
	}

	/**
	*
	*/
	function apply_ga_attributtion_credit() {
		document.querySelector('.modal-credit #input_atributtion_credit_text').select();
		document.execCommand('copy');
		if (ga_credit_arguments_copy) {
			ga(ga_credit_arguments_copy[0], ga_credit_arguments_copy[1], ga_credit_arguments_copy[2], ga_credit_arguments_copy[3]);
		}
	}

	function apply_ga_download_premium() {
		ga(ga_arguments_premium[0], ga_arguments_premium[1], ga_arguments_premium[2], ga_arguments_premium[3]);
		mixpanel.register(mix_panel_json_premium);
	}

	function open_attribution_modal() {
		var modal_credit = document.querySelector('div.modal-credit');
		if ((!gr.auth || gr.auth.me.premium === 0) && modal_credit) {
			modal_credit.checked = true;
			return true;
		} else {
			return false;
		}
	}


</script>

		<script id="tpl-icon" type="text/x-handlebars-template">
	<li class="icon{{#if page}} init page-handler{{/if}} {{#isPersonalIcon premium}} personal-icon{{/isPersonalIcon}}"
		data-id="{{id}}"
		data-name="{{keyword}}"
		data-color="{{color}}"
		data-premium="{{premium}}"
		data-pack="{{pack_slug}}"
		data-search="{{dataSearch}}"
		data-author="{{group_id}}"
	    data-author_name="{{team_name}}"
	    data-author_website="https://www.flaticon.com/authors/{{group_slug}}"
	    data-author_avatar="https://image.flaticon.com/teams/slug/{{group_slug}}.jpg"
	    data-selection="{{selection}}"
	    data-style="{{style_id}}"
	    {{#if page}} data-page="{{page}}"{{/if}}>
	    <div class="icon--holder">
	        <img src="{{png}}" srcset="{{retinaImage}} 4x" alt="{{description}}" title="{{description}}" width="64" height="64">
	        <div class="overlay" aria-hidden="true">
	            <a href="{{slug}}" class="view link-icon-detail" title="{{description}}" data-id="{{#if id}}{{id}}{{/if}}"></a>
	            <button class="add-remove btn flaticon-collections_add nostyle"></button>
	        </div><!-- .overlay -->

	        <div class="popover icon-options hToggleRender">
	            <div class="popover-container">
	                <button class="btn popover-button">
	                    <i class="flaticon-dots"></i>
	                </button>
	                <div class="popover-content">
	                    <ul>
	                        <li  data-ga_params="contextmenu,click,view-details" class="GA_CM_view-details"><a href="{{slug}}" class="link-icon-detail" title="{{description}}" data-id="{{#if id}}{{id}}{{/if}}"><i class="flaticon-eye"></i><span> View detail</span></a></li>
	                        <li  data-ga_params="contextmenu,click,add-to-collection" class="GA_CM_add-to-collection"><a href="#" class="collection_add-remove"><b><i class="flaticon-collections_add"></i></b><span> Add to collection</span></a></li>
	                        <li  data-ga_params="contextmenu,click,edit" class="GA_CM_edit hidePhone {{#if premium}}fi-premium{{else}} {{/if}}"><a href="#" class="context_btn-edit-icon"><i class="flaticon-pencil"></i><span> Edit</span></a></li>
	                        <hr class="{{#if premium}}fi-premium{{else}} {{/if}}">
	                        <li  data-ga_params="contextmenu,click,download-svg" class="GA_CM_download-svg {{#if premium}}fi-premium{{else}} {{/if}}"><a href="#" onclick="direct_download_icon_post(event, 'svg');"><span>Download SVG</span></a></li>
	                        <li  data-ga_params="contextmenu,click,download-png" class="GA_CM_download-png {{#if premium}}fi-premium{{else}} {{/if}}"><a href="#" onclick="direct_download_icon_post(event, 'png');"><span>Download PNG</span></a></li>
	                        <hr>
	                        <li  data-ga_params="contextmenu,click,more-icons-authors" class="GA_CM_more-icons-authors"><a href="#" onclick="document.location='https://www.flaticon.com/search?author_id={{group_id}}';"><span>More icons from this author</span></a></li>
	                        {{#isEqualTo style_id "0"}}
	                        {{else}}
	                        <li  data-ga_params="contextmenu,click,more-icons-style" class="GA_CM_more-icons-style"><a href="#" onclick="document.location='https://www.flaticon.com/search?style_id={{style_id}}';"><span>Search icons with this style</span></a></li>
	                        {{/isEqualTo}}
	                        {{#if pack_slug}}
	                        <li  data-ga_params="contextmenu,click,brownse-pack" class="GA_CM_brownse-pack"><a href="#" onclick="document.location='https://www.flaticon.com/{{pack_slug}}';")><span>Browse pack</span></a></li>
	                        {{/if}}
	                        <li  data-ga_params="contextmenu,click,search-related-icon" class="GA_CM_search-related-icon"><a href="#" onclick="document.location='https://www.flaticon.com/free-icons/{{description}}';"><span>Search related icons</span></a></li>
	                    </ul>
	                </div>
	            </div>
	        </div>
			{{#specialItem premium selection}}
	        <span class="tooltip tooltip--bottom">
	            <span class="tooltip__trigger tooltip__trigger--always modal-trigger flaticon-{{#if premium}}premium{{else}}selection{{/if}} {{#if premium}}premium{{else}}selection{{/if}}" data-modal="modal-selection" onclick="ga('send', 'event', 'Plans', 'What is this?')"></span>
	            <div class="tooltip__content tooltip__content--permanent">
