<script>
$(function() {
    /**
     * Popup
     */
    try {
        var popups = Array.prototype.slice.call(document.getElementsByClassName('popups'));

        popups.forEach(function(popup) {
            var popupModalClose = popup.querySelector('.modal__close');
            var popupModalInterval;
            var popupCount = parseFloat(Cookie.get(popup.id + '_count') || '0');
            var popupLink = popup.querySelector('a');
            var popupMax = parseFloat(popup.getAttribute('data-max'));
            var currentDate = new Date();
            var startDate = new Date(popup.getAttribute('data-start'));
            var endDate = new Date(popup.getAttribute('data-end'));

            /**
             * Events
             */
            var eventLoad = popup.dataset.onload;
            var eventOpen = popup.dataset.open;
            var eventClose = popup.dataset.close;

            if ( (!Cookie.get(popup.id)) && 
                (popup) && 
                (popupLink) &&
                (popupModalClose) && 
                (currentDate.getTime() >= startDate.getTime() && currentDate.getTime() <= endDate.getTime()) && 
                ('undefined' !== typeof gr && !gr.auth.me.premium) && window.innerWidth >= 768 && 
                (!popupMax || popupCount < popupMax) &&
                (eventLoad && eventOpen && eventClose) ) {

                $(popup).addClass('show');

                document.querySelector('body').classList.add('blur');

                if ('popup-blackfriday' === popup.id) {
                    setTimeout(function() {
                        $(popup).addClass('scene--light-flickr');
                    }, 1000);

                    setTimeout(function() {
                        $(popup).addClass('scene--state4');
                    }, 2500);
                }

                ga.apply(null, eventLoad.split(','));
            
                setCountdown(popup, popupModalInterval, startDate, endDate);

                popupModalInterval = setInterval(function() {
                    setCountdown(popup, popupModalInterval, startDate, endDate);
                }, 1000);

                popupLink.addEventListener('click', function() {
                    ga.apply(null, eventOpen.split(','));
                });

                popupModalClose.addEventListener('click', function () {
                    var cookieTime = new Date();
            
                    cookieTime.setHours(cookieTime.getHours() + 3);
            
                    $(popup).removeClass('show');
                    document.querySelector('body').classList.remove('blur');
            
                    Cookie.setH(popup.id, '1', { expires: cookieTime, path: '/' });
                    
                    if (popupMax) Cookie.setH(popup.id + '_count', ++popupCount,  { expires: endDate, path: '/' });

                    ga.apply(null, eventClose.split(','));
            
                    clearInterval(popupModalInterval);
                });
            }
        });


    } catch(err) {
        console.error('There has been an error showing current popups: ', err);
    }


});

</script>	<!-- Start Footer Banner -->


