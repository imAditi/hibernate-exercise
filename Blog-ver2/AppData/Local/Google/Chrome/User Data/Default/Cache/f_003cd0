<script type="text/javascript">
    try {
        var recommendatios = Array.prototype.slice.call(document.getElementsByClassName('recommendations'));

        recommendatios.forEach(function(recommendation) {
            var currentDate = new Date();
            var startDate = new Date(recommendation.getAttribute('data-start'));
            var endDate = new Date(recommendation.getAttribute('data-end'));
            var buttonClose = recommendation.querySelector('.recommendations__close');
            var recommendationInterval;
            var recommendationLink = recommendation.querySelector('a');

            /**
             * Events
             */
            var eventLoad = recommendation.dataset.onload;
            var eventOpen = recommendation.dataset.open;
            var eventClose = recommendation.dataset.close;

            if ( !Cookie.get(recommendation.id) &&
                recommendationLink &&
                (currentDate.getTime() >= startDate.getTime() && currentDate.getTime() <= endDate.getTime()) && 
                (window.innerWidth >= 768) &&
                ('undefined' !== typeof gr && !gr.auth.me.premium) &&
                (eventLoad && eventOpen && eventClose)) {

                ga.apply(null, eventLoad.split(','));

                recommendation.style.display = 'inherit';

                setCountdown(recommendation, recommendationInterval, startDate, endDate);

                recommendationInterval = setInterval(function () {
                    setCountdown(recommendation, recommendationInterval, startDate, endDate);
                }, 1000);

                recommendationLink.addEventListener('click', function () {
                    ga.apply(null, eventOpen.split(','));
                });

                if (buttonClose) {
                    buttonClose.addEventListener('click', function() {
                        recommendation.style.display = 'none';

                        Cookie.setH(recommendation.id, '1', {
                            expires: new Date(new Date().getTime() + (1 * 60 * 60 * 1000)),
                            path: '/'
                        });

                        ga.apply(null, eventClose.split(','));
                    });
                }
            }
        });
    } catch(err) {}
</script>
